   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="de.unisb.cs.esee.ui.activateActionSet"
            label="eSee: Activate"
            visible="true">
         <menu
               id="de.unisb.cs.esee.ui.activateActionSet.menu"
               label="eSee">
            <separator
                  name="eseegroup">
            </separator>
         </menu>
